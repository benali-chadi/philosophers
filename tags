main	philo.c	/^int main (int ac, char **av)$/;"	f
n_eat	philo.h	/^	int		n_eat;$/;"	m	struct:s_philo
num	philo.h	/^	int		num;$/;"	m	struct:s_philo
num_must_eat	philo.h	/^	int	num_must_eat;$/;"	m	struct:s_options
num_of_philos	philo.h	/^	int	num_of_philos;$/;"	m	struct:s_options
philo_routine	philo.c	/^void	*philo_routine(void *arg)$/;"	f
philo_t	philo.h	/^	pthread_t	philo_t;$/;"	m	struct:s_philo
s_options	philo.h	/^typedef struct	s_options {$/;"	s
s_philo	philo.h	/^typedef	struct	s_philo {$/;"	s
t_options	philo.h	/^}		t_options;$/;"	t	typeref:struct:s_options
t_philo	philo.h	/^}		t_philo;$/;"	t	typeref:struct:s_philo
time_to_die	philo.h	/^	int	time_to_die;$/;"	m	struct:s_options
